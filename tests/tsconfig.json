{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "Node",
		"lib": ["ES2020"],
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	},
	"include": ["./**/*.ts"],
	"exclude": ["../dist", "../node_modules"]
}


